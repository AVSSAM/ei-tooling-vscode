{
	"name": "synapse-vscode",
	"displayName": "synapse-vscode",
	"description": "",
	"publisher": "Sajinie",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:xml"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "SynapseXml",
				"extensions": []
			}
		],
		"grammars": [
			{
				"language": "SynapseXml",
				"scopeName": "synapse.xml",
				"path": "./syntaxes/xml.tmLanguage.json"
			}
		],
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"package": "npm run compile && vsce package"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42",
		"vsce": "^1.36.2"
	},
	"dependencies": {
		"vscode-languageclient": "^4.1.4"
	}
}
